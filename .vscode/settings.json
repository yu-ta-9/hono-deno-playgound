    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#fd791b",
    "statusBar.background": "#e36002",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#fd791b",
    "statusBarItem.remoteBackground": "#e36002",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#e36002",
